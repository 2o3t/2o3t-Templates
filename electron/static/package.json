{
    "name": "2o3t-pokemon",
    "version": "0.0.1",
    "author": "zyao89 <zyao89@gmail.com>",
    "description": "An electron project",
    "license": "MIT",
    "main": "./electron/main.js",
    "scripts": {
    },
    "build": {
        "productName": "2o3t-pokemon",
        "appId": "cn.2o3t.app.pokemon",
        "copyright": "Zyao89",
        "directories": {
            "output": "../dist"
        },
        "files": [
          "electron/**/*",
          "icons/**/*",
          "assets/**/*",
          "preload/**/*",
          "package.json",
          "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
          "!**/node_modules/.bin",
          "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
          "!**/._*",
          "!.editorconfig",
          "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.flowconfig,.yarn-metadata.json,.idea,.vs,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "icons/icon.icns",
            "category": "public.app-category.developer-tools",
            "artifactName": "${productName}_setup_${version}.${ext}"
        },
        "win": {
            "icon": "icons/icon.ico",
            "artifactName": "${productName}_setup_${version}.${ext}"
        },
        "linux": {
            "icon": "icons",
            "artifactName": "${productName}_setup_${version}.${ext}"
        }
    },
    "dependencies": {
        "axios": "^0.18.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "vue": "^2.6.10",
        "vue-electron": "^1.0.6",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.1",
        "vuex-electron": "^1.0.3"
    },
    "devDependencies": {
    }
}
