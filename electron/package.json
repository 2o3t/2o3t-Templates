    "private": true,
    "main": "./build/electron/main.js",
    "scripts": {
        "build": "npm run build:electron && electron-builder --projectDir ./build/",
        "build:dir": "npm run build:electron && electron-builder --dir --projectDir ./build/",
        "build:electron": "node .webpack/build.js",
        "build:clean": "cross-env BUILD_TARGET=clean node .webpack/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .webpack/build.js",
        "dev": "node .webpack/dev.js",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .webpack/webpack.config.main.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .webpack/webpack.config.renderer.js",
        "postinstall": "npm run lint:fix"
    },
    "dependencies": {
